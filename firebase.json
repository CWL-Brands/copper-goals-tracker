{
  "hosting": [
    {
      "site": "kanvaportal-goals",
      "source": ".",
      "frameworksBackend": {
        "region": "us-central1"
      },
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Content-Security-Policy",
              "value": "frame-ancestors 'self' https://app.copper.com https://*.copper.com"
            },
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        }
      ]
    },
    {
      "site": "kanvaportal",
      "public": "public",
      "rewrites": [
        {
          "source": "/__/auth/**",
          "destination": "https://kanvaportal-goals.firebaseapp.com/__/auth/**"
        }
      ]
    }
  ],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "source": "functions"
  },
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  }
}